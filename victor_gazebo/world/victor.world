<?xml version="1.0" ?>
<sdf version="1.4">
  <world name="default">
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <physics name="ode" type="ode">
        <ode>
            <constraints>
                <contact_max_correcting_vel>0</contact_max_correcting_vel>
            </constraints>
        </ode>
        <max_step_size>0.001</max_step_size>
        <real_time_update_rate>1000</real_time_update_rate>
    </physics>

  </world>
</sdf>